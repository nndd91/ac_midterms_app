<h1>Doctors#index</h1>
<p>Find me in app/views/doctors/index.html.erb</p>
<div class="panel panel-default">
  <table class="table">
  <% @doctors.each_with_index do |doctor, index| %>
    <tr>
      <td><%= index+1 %>
    </td>
      <td>
        <%= link_to doctor.first_name, doctor_path(doctor) %>
      </td>
    </tr>
  <% end %>
  </table>
</div>
